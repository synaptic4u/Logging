{
    "name": "synaptic4u/logging",
    "version": "1.2",
    "description": "Custom logging class able to log to files and database",
    "type": "library",
    "keywords": [
      "mysql",
      "logging",
      "database"
    ],
    "homepage": "https://github.com/synaptic4u/",
    "require": {
      "synaptic4u/db": "^1.0.1",
      "php": "^7.4",
      "ext-pdo": "*"
    },
    "autoload": {
      "psr-4": {
        "Synaptic4U\\": "src/"
      }
    },
    "scripts": {
      "post-install-cmd": [
        "@create-logs-dir"
      ],
      "create-logs-dir": [
        "[ -d logs ] || mkdir -p logs",
        "chmod 775 logs"
      ]
    },
